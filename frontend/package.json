{
  "name": "portal",
  "version": "1.0.1",
  "author": "Sergey Leleko",
  "private": true,
  "scripts": {
    "dev:admin": "nuxt --config-file src/admin/nuxt.config.js -p=4000",
    "dev:site": "nuxt --config-file src/site/nuxt.config.js -p=4100",
    "build:admin": "nuxt build --config-file src/admin/nuxt.config.js",
    "build:site": "nuxt build --config-file src/site/nuxt.config.js",
    "generate:admin": "nuxt generate --config-file src/admin/nuxt.config.js",
    "generate:site": "nuxt generate --config-file src/site/nuxt.config.js",
    "start:admin": "nuxt start --config-file src/admin/nuxt.config.js -p=4000",
    "start:site": "nuxt start --config-file src/site/nuxt.config.js -p=4100",
    "dev": "concurrently 'yarn dev:admin' 'yarn dev:site'",
    "build": "concurrently 'yarn build:admin' 'yarn build:site'",
    "generate": "concurrently 'yarn generate:admin' 'yarn generate:site'",
    "start": "concurrently 'yarn start:admin' 'yarn start:site'",
    "lint": "eslint --ext .js,.vue --ignore-path ../.gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path ../.gitignore ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/auth": "^4.8.1",
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^2.6.0",
    "bootstrap": "^4.2.0",
    "bootstrap-vue": "^2.1.0",
    "concurrently": "^5.3.0",
    "filepond": "^4.19.2",
    "filepond-plugin-file-encode": "^2.1.6",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "node-sass": "^4.12.0",
    "nuxt": "^2.0.0",
    "nuxt-izitoast": "^0.1.2",
    "sass-loader": "^7.1.0",
    "vue-filepond": "^6.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  }
}
